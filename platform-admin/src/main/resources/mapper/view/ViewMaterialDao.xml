<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.platform.modules.view.dao.ViewMaterialDao">

    <sql id="selectSql">
        SELECT
        T.FMaterialGroupID groupid,T.*
        FROM view_srm_material T
        WHERE 1=1
    </sql>

    <select id="queryAll" resultType="com.platform.modules.view.entity.ViewMaterialEntity">
        <include refid="selectSql"/>
    </select>

</mapper>