<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.platform.modules.qkjvip.dao.MemberVisitDao">
	<select id="selectMemberVisitList" resultType="com.platform.modules.qkjvip.entity.MemberVisitEntity">
		SELECT mv.* FROM qkjvip_member_visit mv
		WHERE 1=1
		<if test="params.uuid != null and params.uuid != ''">
			AND mv.uuid = #{params.uuid}
		</if>
<!--		<if test="params.memberId != null and params.memberId != ''">-->
<!--			AND mv.member_id = #{params.memberId}-->
<!--		</if>-->
		<if test="params.mobile != null and params.mobile != ''">
			AND mv.mobile = #{params.mobile}
		</if>
	</select>
</mapper>